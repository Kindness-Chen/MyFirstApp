<?xml version="1.0" encoding="utf-8"?>
<resources>
       <declare-styleable name="LockPatternView">
        <!-- Aspect to use when drawing LockPatternView. 
        	"square"(default), "lock_width" or "lock_height" -->
        <attr name="aspect" format="string" />
        
        <attr name="line_alpha" format="float" />
        <attr name="line_width" format="dimension" />
        <attr name="normal_line_color" format="color" />
     	<attr name="wrong_line_color" format="color" />
     	
     	<attr name="background_normal_circle" format="reference" />
     	<attr name="background_right_circle" format="reference" />
     	<attr name="background_wrong_circle" format="reference" />
    </declare-styleable>
</resources>
